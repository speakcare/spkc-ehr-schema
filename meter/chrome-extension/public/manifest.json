{
  "manifest_version": 3,
  "name": "SpeakCare",
  "description": "Tracks user session duration and activity on EHR applicatiobns.",
  "version": "0.1.11",
  "icons": {
    "16": "icons/lens-icon-16.png",
    "48": "icons/lens-icon-48.png",
    "128": "icons/lens-icon-128.png"
  },
  "permissions": [
    "sidePanel",
    "scripting",
    "cookies",
    "storage",
    "webRequest",
    "tabs",
    "identity"
  ],
  "oauth2": {
    "client_id": "289656832978-c3bu3104fjasceu6utpihs065tdig833.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets"
    ]
  },
  "host_permissions": [
    "https://*.pointclickcare.com/*"
  ],
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.pointclickcare.com/*"
      ],
      "include_globs": [
        "https://usnpint.pointclickcare.com/*",
        "https://www??.pointclickcare.com/*"
      ],
      "js": [
        "content.bundle.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "side_panel": {
    "default_path": "panel.html"
  },
  "action": {
    "default_icon": {
      "16": "icons/lens-icon-16.png",
      "48": "icons/lens-icon-48.png",
      "128": "icons/lens-icon-128.png"
    },
    "default_title": "Click to open SpeakCare session meter panel"
  },
  "key": ""
}